import urllib2

iniPath = os.getcwd()
sys.path.append(iniPath+'/module')
from mbahSearch import websearch
from mbahCheck import wwwcheck

ws1 = websearch()
ws0 = websearch()

                       
def gSearch(kw):
    ws0.keyword = "inurl:"+kw
    ws0.stop = 200
    for url in ws0.google():
        print "gsearch:-- "+url
        
            
def otherSearch(kw):
    ws1.keyword = kw
    for url in ws1.yahoobing():
        magentoCek(url,kw)


def getkw(saddr):
    req = urllib2.Request(saddr)
    response = urllib2.urlopen(req)
    mesg = response.read()
    return json.loads(mesg)


def resp(saddr, smesg):
    headers = { 'User-Agent' : 'tukang ngantara 0.1' }
    data = urllib.urlencode(smesg)
    req = urllib2.Request(saddr, data, headers)
    return json.loads(urllib2.urlopen(req))


if __name__ == '__main__':
    while True:
        
        #otherSearch(kw)
        gSearch(kw)
